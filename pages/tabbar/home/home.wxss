.nav-wrap {
  position: fixed;
  width: 100%;
  top: 0;
  left: 0;
  z-index: 999;
}

.navbar {
  position: relative;
  top: 0;
  left: 0;
  z-index: 10;
  width: 100%;
  overflow: hidden;
  flex-shrink: 0;
  background: #ffffff;
}

.navbar-title {
  width: 100%;
  box-sizing: border-box;
  padding-left: 115px;
  padding-right: 115px;
  height: 32px;
  line-height: 32px;
  text-align: center;
  position: absolute;
  left: 0;
  z-index: 10;
  color: #333;
  font-size: 16px;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.navbar-action-wrap {
  overflow: hidden;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  -webkit-align-items: center;
  align-items: center;
  position: absolute;
  height: 32px;
  z-index: 11;
  line-height: 1;
  padding: 0 12rpx;
}

.navbar-action-wrap image {
  width: 40rpx;
  height: 40rpx;
}

/*  */
.my_swiper {
  display: block;
}

.club_wrapper {
  position: relative;
  margin: 0 auto;
  margin-top: 60rpx;
  width: 690rpx;
  height: 287rpx;
  box-sizing: border-box;
  padding: 28rpx 0 28rpx 30rpx;
  background: #FFFFFF;
  box-shadow: 0 0px 40rpx 0 rgba(51, 51, 51, 0.1);
  border-radius: 10rpx;
}

.club_wrapper>view:nth-child(-n+2) {
  margin-bottom: 24rpx;
}

.club_wrapper .club_title {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  word-break: break-all;
  font-size: 36rpx;
  font-family: PingFang SC;
  font-weight: 800;
  color: #222222;
}

.phone {
  margin-bottom: 18rpx;
}

.club_wrapper .business,
.phone {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #999999
}

.address {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  word-break: break-all;
}

.address .add_detail {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}

.address .location_icon {
  width: 26rpx;
  height: 26rpx;
  vertical-align: middle;
}

.address .bottom_arrow {
  margin-left: 16rpx;
  width: 26rpx;
  height: 26rpx;
  vertical-align: middle;
}

.store_logo {
  position: absolute;
  top: 28rpx;
  right: 38rpx;
  width: 142rpx;
  height: 142rpx;
}

.nav_wrapper {
  display: flex;
  justify-content: space-between;
  padding: 0 30rpx;
  margin: 60rpx 0 30rpx 0;
}

.nav_left image {
  width: 50rpx;
  height: 50rpx;
  vertical-align: middle;
}

.nav_left text {
  margin-left: 16rpx;
  font-size: 34rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
}

.nav_right text {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #939393;
}

.nav_right image {
  width: 30rpx;
  height: 30rpx;
  vertical-align: middle;
}

.coach_list {
  padding: 0 30rpx;
}

.scroll_x {
  height: auto;
  white-space: nowrap;
  overflow: hidden;
}

.scroll_x .scroll-item {
  display: inline-block;
  width: 256rpx;
  height: 300rpx;
  padding: 20rpx 0 40rpx;
  margin: 0 15rpx;
  /* scroll-view横向滚动的时候，元素没有对齐 */
  vertical-align: top;
}

.coach_item {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
  height: 300rpx;
  background: #FFFFFF;
  box-shadow: 0px 0px 40rpx 0px rgba(51, 51, 51, 0.1000);
  border-radius: 10rpx;
}

.coach_item>view:first-child {
  padding-top: 32rpx;
}

.coach_item .coach_image {
  width: 78rpx;
  height: 78rpx;
  border-radius: 50%;
  vertical-align: bottom;
}

.coach_item>view {
  text-align: center;
}

.coach_item .coach_name {
  margin-top: 8rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #222222;
}

.coach_item .coach_level {
  margin-top: 16rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #999999;
}

.cancatTa {
  margin-top: 20rpx;
  width: 120rpx;
  height: 44rpx;
  text-align: center;
  line-height: 44rpx;
  background: #0B1030;
  border-radius: 22rpx;
  font-size: 24rpx;
  color: #FEFEFE;
}

.sug-nav {
  margin: 20rpx 0 30rpx !important;
}

.recommend {
  display: flex;
  flex-wrap: wrap;
  padding: 0 30rpx 30rpx;
  justify-content: space-between;
}

.recommend_item {
  width: 330rpx;
  height: auto;
  background: #FFFFFF;
  box-shadow: 0px 0px 30rpx 0px rgba(81, 58, 9, 0.1);
  border-radius: 10rpx;
  padding-bottom: 16rpx;
}

.recommend::after {
  content: '';
  width: 330rpx;
}

.recommend_item .house_pic {
  width: 330rpx;
  height: 220rpx;
  vertical-align: bottom;
  border-radius: 10rpx 10rpx 0 0;
}

.cl {
  display: flex;
  padding: 0 30rpx;
  justify-content: space-between;
  margin-top: 20rpx;
}

.class_level {
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #222222;
}

.sign_up>image {
  width: 20rpx;
  height: 20rpx;
  vertical-align: middle;
}

.sign_up text {
  font-size: 20rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #999999;
}

.price {
  padding-left: 30rpx;
  margin-top: 20rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #FF282C;
}

.buy_btn {
  float: right;
  width: 150rpx;
  height: 50rpx;
  margin: 22rpx 30rpx 24rpx 0;
  line-height: 50rpx;
  text-align: center;
  background: #0B1030;
  border-radius: 25rpx;
  color: #FFFFFF;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 800;
}

.active_card {
  display: flex;
  justify-content: space-between;
  width: 690rpx;
  margin: 0 auto;
  margin-bottom: 30px;
}

.active_card>view {
  position: relative;
}

.active_card image {
  width: 334rpx;
  height: 210rpx;
}

.active_card .card_item {
  position: absolute;
  top: 0;
  left: 0;
  z-index: 100;
  width: 334rpx;
  height: 210rpx;
}

.card_year {
  margin: 28rpx 0 0 28rpx;
  font-size: 34rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #EFC37D;
}

.price_btn {
  margin: 80rpx 0 0 180rpx;
  width: 110rpx;
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  background: linear-gradient(90deg, #FBD4A6, #DDA66E);
  border-radius: 15rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #30343C;
}