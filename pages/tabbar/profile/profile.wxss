page {
  font-family: PingFang SC;
}

.navbar {
  width: 100%;
  overflow: hidden;
  position: relative;
  top: 0;
  left: 0;
  z-index: 110;
  flex-shrink: 0;
  background: transparent;
}

.navbar_title {
  position: fixed;
  left: 0;
  z-index: 110;
  width: 100%;
  box-sizing: border-box;
  padding-left: 115px;
  padding-right: 115px;
  height: 32px;
  line-height: 32px;
  text-align: center;
  color: #ffffff;
  font-size: 16px;
  /* font-weight: bold; */
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.profile_bg {
  position: absolute;
  width: 100%;
  top: 0;
  left: 0;
  z-index: 100;
}

.profile_bg image {
  width: 100%;
  height: 494rpx;
}

.profile_top {
  position: absolute;
  top: 78px;
  left: 0;
  width: 100%;
}

.profile_wrapper {
  display: flex;
  padding: 0 30rpx;
  justify-content: space-between;
  align-items: center;
}

.profile_top .profile_arrow {
  width: 36rpx;
  height: 36rpx;
}

.base_info {
  display: flex;
  align-items: center;
  color: #ffffff;
}

.base_info image {
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
  vertical-align: middle;
}

.info_detail {
  margin-left: 30rpx;
}

.phone {
  margin-top: 15rpx;
}

.unbindPhone {
  margin-left: 30rpx;
}

.unbindPhone .user_name {
  font-size: 34rpx;
  font-family: PingFang SC;
  color: #FFFFFF;
}

.unbindPhone .bind_btn {
  margin-top: 16rpx;
  padding: 0 30rpx;
  height: 40rpx;
  line-height: 40rpx;
  background: #FFFEFC;
  border-radius: 20rpx;
  text-align: center;
  font-size: 22rpx;
  font-family: PingFang SC;
  color: #0C1131;
}

/* 卡信息 */
.base_card {
  padding: 0 30rpx;
  margin-top: 116rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  color: #0B1030;
}

.base_card>view:first-child {
  padding: 0 30rpx;
}

.concatMe {
  margin-top: 124rpx;
  margin-left: 80rpx;
  width: 180rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  background: #383241;
  border-radius: 30rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #E9BA85;
}

.card_date {
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  padding: 0 30rpx;
  font-size: 26rpx;
}

.base_card .card_day {
  padding: 0 20rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  background: #373240;
  border-radius: 25rpx;
  color: #ffffff;
  font-size: 24rpx;
}

.category {
  position: relative;
  width: 100%;
  top: 378rpx;
  left: 0;
}

.categiry_item {
  display: flex;
  margin: 0 auto;
  margin-top: 30rpx;
  width: 690rpx;
  height: 143rpx;
  background: #FFFFFF;
  box-shadow: 0px 0px 40rpx 0px rgba(51, 51, 51, 0.1);
  border-radius: 10rpx;
  font-family: PingFang SC;
  color: #333333;
}

.categiry_item>view {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: 50%;
}

.categiry_item>.stored {
  position: relative;
}

.categiry_item>.stored::after {
  position: absolute;
  top: 40rpx;
  right: 0;
  content: '';
  width: 1px;
  height: 60rpx;
  background: #DEDEDE;
}

.number {
  font-size: 34rpx;
}

.nav {
  padding: 54rpx 30rpx 28rpx;
  font-size: 34rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
}

.schedule {
  padding: 30rpx;
}

.schedule image {
  width: 40rpx;
  height: 40rpx;
  vertical-align: middle;
}

.schedule_title {
  margin-left: 30rpx;
  font-size: 34rpx;
  font-family: PingFang SC;
  color: #333333;
}

.schedule {
  display: flex;
  justify-content: space-between;
  align-items: center;
  /* background:pink; */
}

.schedul>view image {
  width: 24rpx;
}

.exitBtn {
  margin: 0 auto;
  margin-top: 110rpx;
  width: 630rpx;
  height: 60rpx;
  line-height: 60rpx;
  background: #FA6C57;
  border-radius: 30rpx;
  text-align: center;
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
}